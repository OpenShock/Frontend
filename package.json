		"@sveltejs/vite-plugin-svelte": "^3.1.0",
		"@tailwindcss/forms": "0.5.7",
		"@tailwindcss/typography": "0.5.13",
		"@types/crypto-js": "^4.2.2",
		"@types/eslint": "^8.56.10",
		"@types/node": "20.12.8",
		"@types/w3c-web-serial": "^1.0.6",
