		"@sveltejs/adapter-node": "^5.0.1",
		"@sveltejs/kit": "^2.5.6",
		"@sveltejs/vite-plugin-svelte": "^3.1.0",
		"@tailwindcss/forms": "0.5.7",
		"@tailwindcss/typography": "0.5.12",
		"@types/crypto-js": "^4.2.2",
		"@types/eslint": "8.56.6",
		"@types/node": "20.12.7",
		"@types/w3c-web-serial": "^1.0.6",
